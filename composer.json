{
    "name": "brunocfalcao/zahper",
    "description": "Zahper MJML Template generator",
    "keywords": [
        "brunocfalcao",
        "zahper",
        "mjml"
    ],
    "homepage": "https://github.com/brunocfalcao/zahper",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bruno Falc√£o",
            "email": "bruno.c.falcao@brunofalcao.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0",
        "guzzlehttp/guzzle": "^7.8.1"
    },
    "autoload": {
        "psr-4": {
            "Brunocfalcao\\Zahper\\": "src"
        },
        "files":
            ["src/helpers.php"]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brunocfalcao\\Zahper\\ZahperServiceProvider"
            ]
        }
    }
}
